<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Rendering -->
    <bean class="arch.galaxyeclipse.client.window.ClientWindow"/>

    <!-- Network -->
    <bean class="arch.galaxyeclipse.client.network.ClientNetworkManager">
        <constructor-arg>
            <bean class="arch.galaxyeclipse.client.network.ClientChannelHandlerFactory"/>
        </constructor-arg>
    </bean>

    <!-- Other -->
    <bean class="arch.galaxyeclipse.client.stage.ui.StageUiFactory"/>
    <bean class="arch.galaxyeclipse.client.resources.ShipStaticInfoHolder"/>
    <bean class="arch.galaxyeclipse.client.resources.LocationInfoHolder"/>
    <bean class="arch.galaxyeclipse.client.resources.CachingResourceLoaderFactory"/>
    <bean class="arch.galaxyeclipse.shared.types.DictionaryTypesMapper"/>
</beans>